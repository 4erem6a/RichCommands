{
  "name": "rich-commands",
  "version": "2.2.1",
  "description": "A command parser",
  "main": "dist/index.js",
  "types": "./types",
  "scripts": {
    "clear": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./scripts/clearDistDirs.ps1",
    "buildRelease": "yarn clear && ./node_modules/.bin/tsc",
    "buildDebug": "yarn clear && ./node_modules/.bin/tsc --inlineSourceMap",
    "docs": "./node_modules/.bin/typedoc --mode file --out docs src",
    "lint": "./node_modules/.bin/eslint src/**",
    "test": "./node_modules/.bin/jest",
    "prepublishOnly": "yarn lint && yarn buildRelease && yarn test"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "rich",
    "commands",
    "command",
    "parser"
  ],
  "author": "4erem6a",
  "license": "MIT",
  "homepage": "https://4erem6a.github.io/RichCommands",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4erem6a/RichCommands.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "typedoc": "^0.15.5",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@4erem6a/inputstream": "^0.1.1"
  }
}
